    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-bh": "ng build && electron .",
    "electron-build": "ng build --prod && electron .",
    "electron-package": "ng build --prod && electron-packager . reflex --platform=linux --arch=x64 --icon=assets/icons/logo.png --prune=true --out=release-builds",
    "electron-installer-debian": " electron-installer-debian --src release-builds/reflex-linux-x64/ --dest release-builds/installers/ --arch amd64 ",
    "electron-build-windows": "ng build --prod && electron-packager . reflex --platform=win32 --arch=x64 --icon=src/assets/img/ico.ico --prune=true --out=release-builds",
    "electron-windows": "electron-packager . reflex --platform=win32 --arch=x64 --icon=src/assets/img/ico.ico --prune=true --out=release-builds",
    "electron-installer-exe": "electron-installer-windows --src release-builds/reflex-win32-x64/ --dest release-builds/installers_windows/ --arch amd64"
  },
  "private": true,
